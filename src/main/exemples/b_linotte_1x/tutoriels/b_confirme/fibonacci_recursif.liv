Nombre Fibonacci en recursif :

Principal :
	Rôles :
		n :: nombre
	Actions :
		"Entrez un nombre :" !
		n ?
		parcours Fibo avec n
		n !

Fibo :
	Rôles :
		* n :: nombre
		§ traitement :: nombre <- n - 1
		§ traitement2 :: nombre <- n - 2
	Actions :
		si n < 2 alors reviens
		parcours Fibo avec traitement
		parcours Fibo avec traitement2
		n vaut traitement + traitement2
		reviens
